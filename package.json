{
  "name": "gulp-reduce",
  "version": "0.0.0",
  "description": "A plugin for gulp",
  "keywords": [
    "gulpplugin"
  ],
  "repository": "mrtnbroder/gulp-reduce",
  "author": {
    "name": "Martin Broder",
