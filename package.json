    "mocha": "~1.17.0",
    "istanbul": "~0.2.3",
    "should": "~3.1.0",
    "coveralls": "~2.7.1",
    "gulp": "~3.5.0",
    "gulp-coffee": "~1.4.1",
    "gulp-clean": "~0.2.2"
  },
  "engines": {
    "node": ">=0.10.0"
